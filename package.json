{
  "name": "react-typescript-base",
  "version": "1.0.0",
  "description": "Basic Typescript React base project (I18N, Helmet, HashRouter, redux, persist, loadable)",
  "private": true,
  "homepage": "https://github.com/mharj/react-typescript-base#readme",
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/ --recursive",
    "watch-css": "node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts-ts start",
	"build-js": "react-scripts-ts build",
	"start": "npm-run-all -p watch-css start-js",
	"build": "npm-run-all build-css build-js",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mharj/react-typescript-base.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mharj/react-typescript-base/issues"
  },
  "devDependencies": {
    "@types/i18next": "^11.9.3",
    "@types/i18next-browser-languagedetector": "^2.0.1",
    "@types/jest": "^23.3.9",
    "@types/react": "^16.4.18",
    "@types/react-dom": "^16.0.9",
    "@types/react-helmet": "^5.0.7",
    "@types/react-loadable": "^5.4.1",
    "@types/react-redux": "^6.0.9",
    "@types/react-router-dom": "^4.3.1",
    "i18next": "^12.0.0",
    "i18next-browser-languagedetector": "^2.2.3",
    "node-sass-chokidar": "^1.3.4",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-helmet": "^5.2.0",
    "react-i18next": "^8.3.5",
    "react-loadable": "^5.5.0",
    "react-redux": "^5.1.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts-ts": "^3.1.0",
    "redux": "^4.0.1",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.3.0",
    "typescript": "^3.1.6"
  },
  "dependencies": {
    "cross-fetch": "^2.2.3",
    "npm-run-all": "^4.1.3"
  }
}
