{
  "name": "react-typescript-base",
  "version": "1.0.0",
  "description": "Basic Typescript React base project (I18N, Helmet, HashRouter, redux, persist, loadable)",
  "private": true,
  "homepage": "https://github.com/mharj/react-typescript-base#readme",
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/ --recursive",
    "watch-css": "node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts-ts start && cra-append-sw --mode dev ./public/custom-sw.js",
    "build-js": "react-scripts-ts build && cra-append-sw --skip-compile ./public/custom-sw.js",
    "lint": "tslint \"src/**/*.ts?(x)\"",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mharj/react-typescript-base.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mharj/react-typescript-base/issues"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/i18next": "^12.1.0",
    "@types/jest": "^24.0.16",
    "@types/jest-diff": "^20.0.1",
    "@types/react": "^16.8.24",
    "@types/react-dom": "^16.8.5",
    "@types/react-helmet": "^5.0.8",
    "@types/react-loadable": "^5.5.1",
    "@types/react-redux": "^7.1.1",
    "@types/react-router-dom": "^4.3.4",
    "@types/redux-mock-store": "^1.0.1",
    "@types/uuid": "^3.4.5",
    "cra-append-sw": "^2.6.1",
    "cross-fetch": "^3.0.4",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "i18next": "^17.0.8",
    "i18next-browser-languagedetector": "^3.0.1",
    "node-sass-chokidar": "^1.3.5",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-app-polyfill": "^1.0.1",
    "react-dom": "^16.8.6",
    "react-helmet": "beta",
    "react-i18next": "^10.11.4",
    "react-loadable": "^5.5.0",
    "react-redux": "^7.1.0",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "react-scripts-ts": "^3.1.0",
    "redux": "^4.0.4",
    "redux-mock-store": "^1.5.3",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.3.0",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.5.3",
    "uuid": "^3.3.2"
  },
  "dependencies": {
    "npm-run-all": "^4.1.5"
  }
}
